{
  "name": "chrome-toolbox",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "谷歌浏览器快捷工具箱，源码https://github.com/zhoubangfu/chrome-toolbox",
  "icons": {
    "16": "img/logo128.png",
    "48": "img/logo128.png",
    "128": "img/logo128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "img/logo32.png"
  },
  "permissions": ["browsingData", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["scripts/jsAccept.js"],
      "run_at": "document_end"
    }
  ]
}
