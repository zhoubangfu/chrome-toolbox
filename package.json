{
  "name": "chrome-toolbox",
  "version": "1.0.0",
  "author": {
    "name": "varddd",
    "url": "https://github.com/varddd"
  },
  "homepage": "https://toolbox.varddd.com",
  "license": "MIT",
  "description": "Chrome 浏览器快捷工具箱",
  "dependencies": {
    "antd": "^4.3.0",
    "axios": "^0.19.0",
    "crypto-js": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.47",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-config-alloy": "^3.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "node-sass": "^4.13.0",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.4.1",
    "typescript": "3.9.7"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eslint:fix": "eslint . --fix --ext .ts,.js,.tsx,.jsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{ts,tsx,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
